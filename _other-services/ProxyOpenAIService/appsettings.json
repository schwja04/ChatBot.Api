{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "minimum" : {
        "ClusterId": "openAIServiceCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "openAIServiceCluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:11434"
          }
        }
      }
    }
  }
}
